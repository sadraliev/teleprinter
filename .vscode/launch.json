{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "run bot.js",
            "program": "${workspaceFolder}/bot.js",
            "console": "integratedTerminal",
            "restart": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "BOT_TOKEN": ""
            }
        },
    ],
}